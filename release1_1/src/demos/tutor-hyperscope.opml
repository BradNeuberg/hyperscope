            <outline hs:createdBy="CE" hs:createdOn="Thu 7 Sep 2006 07:37:47 GMT+00:00" hs:label="Jump" hs:left-label-delim="(" hs:nid="0301" hs:right-label-delim=")" text="Jump commands for navigating the structure of a file:">
               <outline hs:createdBy="CE" hs:createdOn="Tue 5 Sep 2006 02:06:58 GMT+00:00" hs:label="" hs:left-label-delim="(" hs:nid="0309" hs:right-label-delim=")" text="Jump (to) Origin (of file)"/>
