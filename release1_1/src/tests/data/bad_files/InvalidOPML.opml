            <outline hs:createdOn="Mon 15 May 2006 06:25:01 GMT+00:00" hs:label="Include" hs:nid="638" text="Include Links">
               <outline hs:createdOn="Mon 15 May 2006 06:29:02 GMT+00:00" hs:label="We" hs:nid="640" text="We will use OPML 2.0s 'include' link type for include links. The 'text' attribute should have the full Augment style INCLUDE syntax in the OPML 'text' attribute for displaying."/>
               <outline hs:createdOn="Mon 15 May 2006 06:32:04 GMT+00:00" hs:label="We will add one custom attribute" hs:nid="655" text="We will add one custom attribute: hs:include-type, which can be of three values: branch, plex, statement">
                  <outline hs:createdOn="Mon 15 May 2006 06:32:10 GMT+00:00" hs:label="If" hs:nid="656" text="If this attribute is left off, it defaults to branch."/>
               </outline>
               <outline hs:createdOn="Mon 15 May 2006 06:29:06 GMT+00:00" hs:label="See http" hs:nid="641" text="See http://www.opml.org/spec2#inclusion for details on how OPML 2.0 inclusion works and is specified."/>
               <outline hs:createdOn="Mon 15 May 2006 06:28:32 GMT+00:00" hs:label="Server-side" hs:nid="642" text="Server-side transforms will need to convert Augment include links into this OPML type (type=inclusion)"/>
            </outline>
            <outline hs:createdOn="Mon 15 May 2006 06:25:03 GMT+00:00" hs:label="Embedded" hs:nid="639" text="Embedded Links">
               <outline hs:createdOn="Mon 15 May 2006 06:34:19 GMT+00:00" hs:label="Augment-style" hs:nid="643" text="Augment-style links must be fully transformed when they reach the client-side."/>
               <outline hs:createdOn="Mon 15 May 2006 06:29:34 GMT+00:00" hs:label="The Augment server-side transformer must change the following Augment-style links into their full HTML style" hs:nid="644" text="The Augment server-side transformer must change the following Augment-style links into their full HTML style:">
                  <outline hs:createdOn="Mon 15 May 2006 06:29:37 GMT+00:00" hs:label="&lt;link&gt;" hs:nid="645" text="&lt;a href=&quot;link&quot;&gt;&amp;lt;link&amp;gt;&lt;/a&gt;"/>
                  <outline hs:createdOn="Mon 15 May 2006 06:29:38 GMT+00:00" hs:label="[link]" hs:nid="646" text="&lt;a href=&quot;link&quot;&gt;[link]&lt;/a&gt;"/>
                  <outline hs:createdOn="Mon 15 May 2006 06:29:39 GMT+00:00" hs:label="(link)" hs:nid="647" text="&lt;a href=&quot;link&quot;&gt;(link)&lt;/a&gt;"/>
                  <outline hs:createdOn="Mon 15 May 2006 06:29:41 GMT+00:00" hs:label="(see--)" hs:nid="648" text="&lt;a href=&quot;see--&quot;&gt;(see--)&lt;/a&gt;">
                     <outline hs:createdOn="Mon 15 May 2006 06:30:38 GMT+00:00" hs:label="Note" hs:nid="649" text="Note: Anything that begins with a word, and is followed by --, is ignored when surrounded by parentheses"/>
                  </outline>
               </outline>
               <outline hs:createdOn="Mon 15 May 2006 06:29:57 GMT+00:00" hs:label="These must be transformed into embedded HTML links, with their Augment style delimiters left" hs:nid="652" text="These must be transformed into embedded HTML links, with their Augment style delimiters left:">
                  <outline hs:createdOn="Mon 15 May 2006 06:30:07 GMT+00:00" hs:label="&lt;a" hs:nid="653" text="&lt;a href=&quot;a href=&amp;quot;link&amp;quot;&gt;&amp;lt;link&amp;gt;&lt;/a&quot;&gt;&amp;lt;a href=&quot;link&quot;&gt;&amp;lt;link&amp;gt;&lt;/a&amp;gt;&lt;/a&gt;"/>
               </outline>
            </outline>
            <outline hs:createdOn="Mon 15 May 2006 06:35:01 GMT+00:00" hs:label="Markers" hs:nid="658" text="Markers">
               <outline hs:createdOn="Mon 15 May 2006 06:35:07 GMT+00:00" hs:label="We" hs:nid="659" text="We need to support Augment markers in OPML."/>
               <outline hs:createdOn="Mon 15 May 2006 06:35:13 GMT+00:00" hs:label="This is done with a new tag located in the OPML head element" hs:nid="660" text="This is done with a new tag located in the OPML head element:">
                  <outline hs:createdOn="Mon 15 May 2006 06:35:43 GMT+00:00" hs:label="&lt;hs" hs:nid="661" text="&lt;a href=&quot;hs:marker id=&amp;quot;markerName&amp;quot; location=&amp;quot;#023+10c&amp;quot; /&quot;&gt;&amp;lt;hs:marker id=&quot;markerName&quot; location=&quot;#023+10c&quot; /&amp;gt;&lt;/a&gt;"/>
               </outline>
               <outline hs:createdOn="Mon 15 May 2006 06:35:42 GMT+00:00" hs:label="Where 'id' is the marker's name, such as 'foobar', and 'location' is a relative Augment style link composed as follows" hs:nid="662" text="Where 'id' is the marker's name, such as 'foobar', and 'location' is a relative Augment style link composed as follows:">
                  <outline hs:createdOn="Mon 15 May 2006 06:35:50 GMT+00:00" hs:label="It" hs:nid="663" text="It begins with an anchor, #, since it is relative and a link"/>
                  <outline hs:createdOn="Mon 15 May 2006 06:36:07 GMT+00:00" hs:label="It" hs:nid="664" text="It is followed by the node ID that contains this marker, such as 023"/>
                  <outline hs:createdOn="Mon 15 May 2006 06:36:36 GMT+00:00" hs:label="Finally," hs:nid="665" text="Finally, it uses string positioning to offset from the beginning of the addressed node; this should be a positive string position offset, and should use character addressing (i.e. +10c, NOT -5w, for example). ">
                     <outline hs:createdOn="Mon 15 May 2006 06:37:14 GMT+00:00" hs:label="Note" hs:nid="666" text="Note: When the server-side transforms these values from other formats that might have embedded HTML, such as XHTML, the string position should correctly refer to text and NOT HTML characters. For example, if I had 'hello &lt;i&gt;world&lt;/i&gt;', and the marker is at the 'w' letter, this address should be +7c, NOT +10c, which is what it would be if we included the HTML in the count. Also, notice that counting starts from 1, not 0. "/>
                  </outline>
               </outline>
               <outline hs:createdOn="Mon 15 May 2006 06:37:29 GMT+00:00" hs:label="There can be as many hs" hs:nid="667" text="There can be as many hs:marker elements as you desire, all embedded in a single hs:markers element in the OPML head element:">
                  <outline hs:createdOn="Mon 15 May 2006 06:37:37 GMT+00:00" hs:label="&lt;head&gt;" hs:nid="668" text="&lt;a href=&quot;head&quot;&gt;&amp;lt;head&amp;gt;&lt;/a&gt;">
                     <outline hs:createdOn="Mon 15 May 2006 06:37:39 GMT+00:00" hs:label="&lt;hs" hs:nid="669" text="&lt;a href=&quot;hs:markers&quot;&gt;&amp;lt;hs:markers&amp;gt;&lt;/a&gt;">
                        <outline hs:createdOn="Mon 15 May 2006 06:37:43 GMT+00:00" hs:label="&lt;hs" hs:nid="670" text="&lt;a href=&quot;hs:marker id=&amp;quot;foobar&amp;quot; location=&amp;quot;#023+10c&amp;quot;/&quot;&gt;&amp;lt;hs:marker id=&quot;foobar&quot; location=&quot;#023+10c&quot;/&amp;gt;&lt;/a&gt;"/>
                        <outline hs:createdOn="Mon 15 May 2006 06:37:48 GMT+00:00" hs:label="&lt;marker" hs:nid="671" text="&lt;a href=&quot;marker id=&amp;quot;hello&amp;quot; location=&amp;quot;#055+50c&amp;quot;/&quot;&gt;&amp;lt;marker id=&quot;hello&quot; location=&quot;#055+50c&quot;/&amp;gt;&lt;/a&gt;"/>
                     </outline>
                     <outline hs:createdOn="Mon 15 May 2006 06:37:49 GMT+00:00" hs:label="&lt;/hs" hs:nid="672" text="&lt;a href=&quot;/hs:markers&quot;&gt;&amp;lt;/hs:markers&amp;gt;&lt;/a&gt;"/>
                  </outline>
                  <outline hs:createdOn="Mon 15 May 2006 06:37:51 GMT+00:00" hs:label="&lt;/head&gt;" hs:nid="673" text="&lt;a href=&quot;/head&quot;&gt;&amp;lt;/head&amp;gt;&lt;/a&gt;"/>
               </outline>
            </outline>
