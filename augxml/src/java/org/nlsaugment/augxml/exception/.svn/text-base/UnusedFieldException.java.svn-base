package org.nlsaugment.augxml.exception;


/**
 * This exception is thrown in cases where fields of type AugUnused are not empty.
 * @author Jonathan Cheyer
 *
 */
public final class UnusedFieldException extends AugmentException {
  private static final long serialVersionUID = 1L;
  public UnusedFieldException(final String message) {
    super(message);
  }

  public UnusedFieldException(final String message, final Exception e) {
    super(message, e);
  }
}
